AAC,YAA4D;AAA/C,QAAA,IAAI,EAAC,SAA0C;AAA/B,QAAA,OAAK,EAAE,MAAA,CAAA;AAAwB,OAA1E,E;0BAA2D;AAAA,iBAAG,C,WAAA,CAAH;AAAA,S;;;;OAA3D,E;;AAAA,Q,WAAA,CACG,CAHP,CAGO,CAHP;AAAA,KADiB,C;sBApBjB;AAAA,aAmBU,CAnBV,YAAA,CAmBU,kBAnBV,EAmBU;AAnBA,QAAA,KAAK,EAAE,IAAA,CAAA;AAmBP,OAnBV,EAA0B;0BAC1B;AAAA,iBAEe,CAFf,YAAA,CAEe,uBAFf,EAEe,IAFf,EAEe;8BADX;AAAA,qBAAkF,CAAlF,YAAA,CAAkF,mBAAlF,EAAkF;AAAxE,gBAAA,KAAkB,EAAlB;AAAA,2BAAA;AAAA,iBAAwE;AAAnD,gBAAA,GAAG,EAAE,MAAA,CAAA,WAAA,CAAY,GAAkC;AAA5B,gBAAA,GAAG,EAAE,IAAA,CAAA,KAAA,GAAM,IAAA,CAAA;AAAiB,eAAlF,E,IAAA,E;;AAAA,gB,cAAA,CAAkF,CAAlF;AAAA,a,CACW;;;;AAAA,WAFf,CAEe,EACf,YAAA,CAEe,uBAFf,EAEe;AAFD,YAAA,KAAK,EAAC;AAEL,WAFf,EAAwB;8BACpB;AAAA,qBAAwF,CAAxF,YAAA,CAAwF,mBAAxF,EAAwF;4BAArE,MAAA,CAAA,WAAA,CAAY,KAAyD;;yBAArE,MAAA,CAAA,WAAA,CAAY,KAAZ,GAAiB,M;kBAAoD;AAAlD,gBAAA,YAAY,EAAC,KAAqC;AAA/B,gBAAA,WAAW,EAAC;AAAmB,eAAxF,E,IAAA,E;;AAAA,gB,cAAA,CAAwF,CAAxF;AAAA,a,CADoB;;;;AAAA,WAAxB,CADe,EAIf,YAAA,CAQe,uBARf,EAQe;AARD,YAAA,KAAK,EAAC;AAQL,WARf,EAAwB;8BACpB;AAAA,qBAME,CANF,YAAA,CAME,uBANF,EAME;4BALW,MAAA,CAAA,WAAA,CAAY,IAKvB;;yBALW,MAAA,CAAA,WAAA,CAAY,IAAZ,GAAgB,M;kBAK3B;AAJG,gBAAA,OAAO,EAAE,MAAA,CAAA,IAIZ;AAHE,gBAAA,WAAW,EAAC,GAGd;AAFE,gBAAA,KAAoB,EAApB;AAAA,2BAAA;AAAA,iBAEF;AADE,gBAAA,QAAQ,EAAR;AACF,eANF,E,IAAA,E;;AAAA,gB,yBAAA,CAME,CANF;AAAA,a,CADoB;;;;AAAA,WAAxB,CAJe,EAaf,YAAA,CAEe,uBAFf,EAEe;AAFD,YAAA,KAAK,EAAC;AAEL,WAFf,EAAwB;8BACpB;AAAA,qBAAsG,CAAtG,YAAA,CAAsG,mBAAtG,EAAsG;4BAAnF,MAAA,CAAA,WAAA,CAAY,WAAuE;;yBAAnF,MAAA,CAAA,WAAA,CAAY,WAAZ,GAAuB,M;kBAA4D;AAAzD,gBAAA,IAAI,EAAC,UAAoD;AAAxC,gBAAA,IAAI,EAAE,CAAkC;AAA/B,gBAAA,WAAW,EAAC;AAAmB,eAAtG,E,IAAA,E;;AAAA,gB,cAAA,CAAsG,CAAtG;AAAA,a,CADoB;;;;AAAA,WAAxB,CAbe,CAFf;AAAA,S,CAD0B;;;;AAAA,OAA1B,E;;AAAA,Q,SAAA,CAmBU,CAnBV;AAAA,K;;;;GADJ,E;;AAAA,I,cAAA,C","sourcesContent":["<template>\n    <div v-if=\"$route.name !== 'Home'\" class=\"zhanwei\"></div>\n    <main class=\"header\">\n    <div class=\"homeHeader\" v-show=\"isShow\">\n    <el-menu  :default-active=\"activeIndex\" class=\"el-menu-demo\" mode=\"horizontal\">\n    <el-menu-item  @click=\"$router.push('/')\" index=\"1\">主页</el-menu-item>    \n    <el-menu-item  @click=\"$router.push('/explore')\" index=\"2\">发现</el-menu-item>\n    <el-menu-item  @click=\"$router.push('/community')\" index=\"3\">广场</el-menu-item>\n    <el-menu-item  @click=\"$router.push('/writing')\" index=\"4\">分享啦~</el-menu-item>\n    </el-menu>\n    <el-tooltip v-if=\"isSignIn\" :content=\"'你已登录:'+userName\" placement=\"bottom\" effect=\"light\">\n    <img   class=\"isImg\" src=\"https://sf1-ttcdn-tos.pstatp.com/obj/larkcloud-file-storage/baas/qc5abu/2dedbafcccf601c4_1630335837047.png\" alt=\"\">\n    </el-tooltip>\n    <div  v-else class=\"userImg\" @click=\"logIn()\"></div>\n    <el-button style=\"margin-right:14px\" size=\"medium\"  type=\"primary\" round @click=\"$router.push('/edit')\">制图</el-button>\n    </div>\n    <el-button  v-show=\"!isShow\" style=\"margin-right:14px\" size=\"medium\"  type=\"primary\" round @click=\"share\" id=\"shareBtn\">分享</el-button>\n    <el-button  v-show=\"!isShow\" style=\"margin-right:14px\" size=\"medium\"  type=\"primary\" round @click=\"download\">保存</el-button>\n    <div  @click=\"logIn()\" class=\"userImg\" v-show=\"!isShow && !isSignIn\"></div>\n    <el-tooltip  v-if=\"!isShow && isSignIn\" :content=\"'你已登录:'+userName\" placement=\"bottom\" effect=\"light\">\n    <img  class=\"isImg\" src=\"https://sf1-ttcdn-tos.pstatp.com/obj/larkcloud-file-storage/baas/qc5abu/2dedbafcccf601c4_1630335837047.png\" alt=\"\">\n    </el-tooltip>\n    <router-link to=\"/\" class=\"logo\">WeChart</router-link>\n    </main>\n    \n\n    <el-dialog title=\"作品分享\" v-model=\"dialogFormVisible\">\n        <el-form :model=\"shareData\">\n        <el-form-item >\n            <el-image style=\"width:80%;\"  :src=\"shareDialog.img\" :fit=\"scale-down\"></el-image>\n        </el-form-item>\n        <el-form-item label=\"标题\">\n            <el-input v-model=\"shareDialog.title\" autocomplete=\"off\" placeholder=\"请输入标题\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"标签\">\n            <el-select-v2\n                v-model=\"shareDialog.tags\"\n                :options=\"tags\"\n                placeholder=\" \"\n                style=\"width: 100%;\"\n                multiple\n            />\n        </el-form-item>\n        <el-form-item label=\"内容\">\n            <el-input v-model=\"shareDialog.description\"  type=\"textarea\" :rows=\"4\" placeholder=\"请输入内容\"></el-input>\n        </el-form-item>\n        </el-form>\n        <template #footer>\n        <span class=\"dialog-footer\">\n            <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\n            <el-button id=\"toShareBtn\" type=\"primary\" @click=\"toShare\">确 定</el-button>\n        </span>\n        </template>\n    </el-dialog>\n</template>\n\n<script>\nimport { onMounted, onUpdated, reactive, ref, watch } from 'vue'\nimport { ElNotification } from 'element-plus';\nimport { useRoute, useRouter} from 'vue-router'\nimport { useStore } from 'vuex'\nimport {shareSave} from '../http/share'\nimport judgeExpired from '../http/judgeExpired'\nimport { ElMessage } from 'element-plus'\nimport downloadFile from '../common/download/download.js'\nexport default {\n    name:'navBar',\n    setup() {\n        const route = useRoute()\n        const router = useRouter()\n        const store = useStore()\n        const activeIndex = ref('0');\n        const isShow = ref(true)\n        const routerNow = {\n                    Home:'1',\n                    Explore:'2',\n                    Community:'3',\n                    BlogDetail:'2',\n                }\n\n        /**\n         * 判断登录\n         */\n        const isSignIn = ref(false)\n        const userName = ref('')\n        onUpdated(async ()=>{\n            try{\n                const res = await judgeExpired({token:localStorage.getItem('token')})\n                if(res.status === '403')\n                if(res.result.data.message === 'jwt expired'){\n                    ElMessage.warning('登陆过期了哦')\n                    store.state.isSignIn = false\n                    isSignIn.value = false\n                    return\n                }\n                if(res.status === '200'){\n                    store.state.isSignIn = true\n                    userName.value = res.result.userName\n                }\n            }\n            catch(err) {\n                ElMessage.error('出错啦！')\n            }\n        })\n        watch(()=>store.state.isSignIn,()=>{\n            isSignIn.value = store.state.isSignIn\n        })\n\n        /**\n         * 判断当前页面是否需要显示主页，发现，广场\n         * 根据路由变化更改当前el-menu的index\n         */\n        watch(\n            ()=>route.name,\n            ()=>{\n                if( new RegExp('(^Edit)|ChartType|ChartTheme').test(route.name))\n                isShow.value = false\n                else\n                isShow.value = true\n\n                if(routerNow[route.name])\n                activeIndex.value = routerNow[route.name]\n            }\n        )\n\n        /**\n         * 优化刷新时的el-menu的index跳转 \n         */\n        onMounted(()=>{\n            if(routerNow[route.name])\n            activeIndex.value = routerNow[route.name]   \n        })\n    \n\n        /**\n         * 保存功能\n         */\n        function download() {downloadFile('chart.png',store.state.myChart.getDataURL())}\n\n\n\n        /**\n         * 更改标签\n        */\n        const tags = ref([\n            {value:'经济',label:'经济'},\n            {value:'农业',label:'农业'},\n            {value:'气候',label:'气候'},\n            {value:'教育',label:'教育'},\n            {value:'能源',label:'能源'},\n            {value:'环境',label:'环境'},\n            {value:'性别',label:'性别'},\n            {value:'健康',label:'健康'},\n            {value:'基础建设',label:'基础建设'},\n            {value:'社会保障',label:'社会保障'},\n        ])\n        /**\n         * 分享功能\n         */\n        const shareDialog = reactive({\n        title:'',\n        img:'',\n        description:'',\n        tags:[]\n        });\n        const dialogFormVisible = ref(false)\n        const shareData = reactive({\n            imageUrl:'default',\n            title:'default',\n            description:'default',\n            tags:[],\n            collect:66,\n            getNum:66,\n            userId:'3035816700@qq.com',\n            userName:'Aei'\n        })\n        async function share() {     \n            if(!store.state.isSignIn){\n                ElMessage.warning('还未登录哦')\n                return\n            }\n            try{\n                const res = await judgeExpired({token:localStorage.getItem('token')})\n                if(res.status === '403')\n                if(res.result.data.message === 'jwt expired'){\n                    ElMessage.warning('登陆过期了哦')\n                    store.state.isSignIn = false\n                    isSignIn.value = false\n                    return\n                }\n                if(res.status === '200'){\n                    shareData.userId = res.result.userId\n                    shareData.userName = res.result.userName\n                }\n            }\n            catch(err) {\n                ElMessage.error('出错啦！')\n            }\n            document.getElementById('shareBtn').disabled = true\n            shareDialog.img = await store.state.myChart.getDataURL()\n            document.getElementById('shareBtn').disabled = false\n            dialogFormVisible.value = true\n        }\n        async function toShare() {\n            document.getElementById('toShareBtn').disabled = true\n            shareData.imageUrl = shareDialog.img\n            shareData.title = shareDialog.title\n            shareData.description = shareDialog.description\n            shareData.tags = shareDialog.tags\n            try{\n               let res = await shareSave(shareData)\n               console.log(res)\n               ElNotification({\n                title: '分享成功',\n                message: '分享内容可在分享大厅查看',   \n                position: 'bottom-right'\n                });\n            }catch(error)\n            {\n                console.error(error)\n                ElNotification({\n                title: '分享失败',\n                message: '分享过程出现意外',\n                type: 'warning',\n                position: 'bottom-right'\n                });\n            }\n            document.getElementById('toShareBtn').disabled = false\n            dialogFormVisible.value = false\n        }\n\n        /**\n         * 登录\n         */\n        async function logIn() {\n            router.push('/login')\n        }\n\n        return {\n        activeIndex,\n        isShow,\n        download,\n        share,\n        shareDialog,\n        tags,\n        dialogFormVisible,\n        toShare,\n        isSignIn,\n        logIn,\n        userName\n        };\n    }\n}\n</script>\n\n<style  lang = \"less\" scoped>\n    a {\n        text-decoration: none;\n    }\n    .zhanwei {\n        height: 60px;\n    }\n    .header {\n        z-index: 7;\n        position: fixed;\n        display: flex;\n        flex-direction: row-reverse;\n        top: 0;\n        align-items:center;\n        background: white;\n        height: 60px;\n        width: 100%;\n        .logo {\n            flex:1;\n            color: #2F80ED;\n            text-align: left;\n            font-size: 24px;\n            margin-left: 18px;\n        }\n        .el-menu-demo {\n            border: 0;\n        }\n        .homeHeader {\n            display: flex;\n            align-items: center;\n        }\n        .userImg {\n            height: 34px;\n            width: 34px;\n            border-radius: 50%;\n            background: #C4C4C4;\n            margin-left: 28px;\n            margin-right: 28px;\n            cursor: pointer;\n        }\n        .isImg {\n            height: 34px;\n            background: white;\n            margin-left: 28px;\n            margin-right: 28px;\n        }\n        .toEdit {\n            height: 36px;\n        }\n    }\n    .el-select-v2__placeholder.is-transparent{\n        transform: translateY(50%) translateX(-98%);\n    }\n\n</style>"],"sourceRoot":""}]}