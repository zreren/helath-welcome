\n      }\n      return{\n          textLink,\n          ...toRefs(data)\n      }\n  }, \n  methods: {\n     \n  }\n}\n</script>\n\n<style scoped lang=\"less\">\n.container::-webkit-scrollbar {display:none}\n.container1{\n    text-align: left;\n    margin: 1vh auto;\n    .content{\n        overflow: hidden;\n        margin-top: 1vh;\n        display: flex;\n        align-items: center;\n       justify-items: center;\n        height:auto;\n        .showImage:hover {cursor:pointer}\n        .showImage{\n            width: 30%;\n            height: 150px;\n         \n            text-align: center;\n            img{\n                height: 100%;\n                width: auto;\n                max-width: 100%;\n            }\n        }\n        .showMessage > *:hover{cursor:pointer}\n        .showMessage{\n            width: 70%;\n            color: #233062;\n            .title{\n                color: #A9B4C1;\n                padding: 1vh 1vw;\n                span{\n                    margin: 0 5px;\n                }\n            }\n            .info{\n                 padding: 1vh 1vw;\n            }\n            .data{\n                padding: 1vh 1vw;\n            }\n        }\n    }\n\n}\n\n</style>"],"sourceRoot":""}]}